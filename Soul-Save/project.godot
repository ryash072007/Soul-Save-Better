; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Soul Save"
run/main_scene="res://Scenes/splash_screen.tscn"
config/features=PackedStringArray("4.0", "Vulkan Mobile")
boot_splash/show_image=false
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://icon.png"

[autoload]

AudioManager="*res://AudioManager.tscn"

[display]

window/size/viewport_height=576
mouse_cursor/custom_image="res://Assets/Mouse/mouse1.png"
window/stretch/mode="viewport"

[rendering]

vulkan/rendering/back_end=1
textures/canvas_textures/default_texture_filter=0
